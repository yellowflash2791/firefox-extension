{
  "manifest_version": 2,
  "name": "PopupBlocker",
  "version": "0.3",
  "description": "Blocks new tab creation",
  "permissions": ["tabs","storage","https://extensionpay.com/*"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "500": "icon128.png"
  },  
  "background": {
//    "scripts": ["background.js","ExtPay.js"], 
    "page": "background.html"	
  }

}

